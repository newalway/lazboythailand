{% extends "ProjectBundle:"~ view_layout ~":layout.html.twig" %}

{% block title %}{% trans %}menu.home{% endtrans %} | {{ parent() }}{% endblock %}
{% block home_active %}active{% endblock %}
{% block navbar_class %}rd-navbar_transparent{% endblock %}
{% block navbar_absolute_class %}rd-navbar-absolute{% endblock %}
{% block breadcrumbs %}{% endblock %}

{% block content %}
<style media="screen">
.carousel-product .owl-stage-outer {
    padding-top: 0px;
    z-index: 1;
}
</style>
<!-- Swiper-->
{% if banners|length > 0 %}
<section class="">
    <div class="owl-carousel carousel-post-gallery carousel-slider-blog-post" id="owl-carousel-banner" data-items="1" data-stage-padding="0" data-loop="true" data-margin="0" data-mouse-drag="true" data-nav="true" data-dots="true" data-photo-swipe-gallery="gallery" data-autoplay="true" data-smart-speed="1000" data-video="true" data-autoplay-hover-pause="true">
        {% for banner in banners %}
        <div class="item text-center {% if banner.getVideo %}video{% endif %}">
            {% if banner.getVideo %}
                {#<a data-fancybox href="{{banner.video}}">
                    <img class="wh img-responsive" src="{{banner.thumbnailUrlPlayButton}}" alt="">
                    <div class="title-category">{{banner.getTitle}}</div>
                </a>#}
                <div class="embed-responsive embed-responsive-21by9">
                    <iframe class="embed-responsive-item embed-player" src="https://www.youtube.com/embed/{{banner.video|youtube}}?rel=0" allowfullscreen></iframe>
                    {#<a class="embed-responsive-item embed-player owl-video" href="https://www.youtube.com/embed/{{banner.video|youtube}}?rel=0"></a>#}
                </div>
            {% elseif banner.image %}
                <a {% if banner.getWebsite %}href="{{banner.getWebsite}}"{% endif %}>
                    <figure>
                        {% set banner_image_type = banner.image|getFileType %}
                        {% if banner_image_type == 'gif' %}
                            {% set banner_image = asset(banner.image) %}
                        {% else %}
                            {% set banner_image = asset(banner.image) | imagine_filter('img_1960x840') %}
                        {% endif  %}
                        <img src="{{banner_image}}" alt="{{banner.getTitle}}" class="w-100"/>
                    </figure>
                </a>
            {% endif %}
        </div>
        {% endfor %}
    </div>
</section>
{% endif %}
<!-- Divider-->
<!-- <div class="row mt-0 mb-0">
    <div class="divider"></div>
</div> -->

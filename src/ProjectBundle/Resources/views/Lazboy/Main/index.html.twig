                            {% endif %}
                        </a>
                    </div>
                    <div class="product-caption">
                        <h6 class="product-title">
                            <a href="{{path('product_category',{'cate_id':product_category.id,'slug':product_category.slug})}}">{{product_category.getTitle}}</a>
                        </h6>
                        {% if product_category.getShortDesc %}
                        <p>{{product_category.getShortDesc}}</p>
                        {% endif %}
                    </div>
                </div>
            </div>
            {% endfor %}
        </div>
    </div>
</section>

<!-- Divider-->
<!-- <div class="container">
    <div class="divider"></div>
</div> -->
{% endif %}


{% if product_categorys|length > 0 %}
<section class="section-sm bg-default pt-0 pb-0 home-pro-cate">
    <div class="container text-center">
        <h4 class="line-text">{% trans %}title.our_product_categories{% endtrans %}</h4>
        <!-- Owl Carousel-->
        <div class="owl-carousel carousel-product p-2" data-items="1" data-md-items="2" data-lg-items="3" data-xl-items="4" data-stage-padding="0" data-loop="false" data-margin="50" data-mouse-drag="false" data-nav="true">
            {% for product_category in product_categorys %}
            <div class="item">
                <div class="product product-grid">
                    <div class="product-img-wrap">
                        <a href="{{path('product_category',{'cate_id':product_category.id,'slug':product_category.slug})}}">
                            {% if product_category.image %}
                            <img src="{{asset(product_category.image) | imagine_filter('img_300x300')}}" alt="{{product_category.getTitle}}" width="300"/>

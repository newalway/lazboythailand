{% if inspiration_categories|length > 0 %}
    {% for inspiration_category in inspiration_categories %}
        {% set top_inspirations = getInspirationByCategoryId(inspiration_category.id)  %}
        {% if top_inspirations|length > 0 %}
            <section class="section-sm bg-default text-center">
                <div class="container">
                    <h4 class="line-text">{{inspiration_category.getTitle|upper}}</h4>
                    <div class="row row-5 justify-content-md-center">
                        <!-- inspirations loop  -->
                        {% for top_inspiration in top_inspirations %}
                            {% set top_inspiration_link = path('inspiration_detail',{'id':top_inspiration.id,'slug':top_inspiration.getTitle|slug}) %}
                            <div class="col-md-6 col-lg-4">
                                <!-- Post classic-->
                                <article class="post-classic post-minimal text-center">
                                    <a href="{{top_inspiration_link}}">
                                        {% if top_inspiration.image %}
                                            <img src="{{asset(top_inspiration.image) | imagine_filter('img_418x259')}}" alt="{{top_inspiration.getTitle}}" width="418" height="259"/>
                                        {% else %}
                                            <img src="{{asset('lazboy/images/img_418x315.png')}}" alt="No Image" width="418" height="259"/>
                                        {% endif %}
                                    </a>
                                    <div class="post-classic-title">
                                        <h6>
                                            <a href="{{top_inspiration_link}}">{{top_inspiration.getTitle}}</a>
                                        </h6>
                                    </div>
                                    <div class="post-meta">
                                        <div class="group">
                                            <a class="meta-author clear-font-style" href="{{path('inspiration_detail',{'id':top_inspiration.id,'slug':top_inspiration.getTitle|slug})}}">{{top_inspiration.getShortDesc}}</a>
                                        </div>
                                    </div>
                                </article>
                            </div>
                        {% endfor %}
                    </div>
                </div>
            </section>
        {% endif %}
    {% endfor %}
{% endif %}


